{
  "name": "happy-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "nodemon dist/server.js",
    "dev": "npm run seed && tsx src/server.ts",
    "seed": "tsx src/database/seed.ts",
    "build": "rollup -c && npm run build:hbs && npm run build:data",
    "build:hbs": "parcel build src/views/*.hbs --out-dir dist/views",
    "build:data": "parcel build src/database/database.sqlite --out-dir dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-hbs": "^2.4.0",
    "leaflet": "^1.9.4",
    "nodemon": "^2.0.5",
    "sqlite-async": "^1.2.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.3.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^20.4.2",
    "parcel-bundler": "^1.12.5",
    "rollup": "^3.26.3",
    "tslib": "^2.6.0",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
